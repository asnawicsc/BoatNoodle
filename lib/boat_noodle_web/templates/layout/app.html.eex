<script>
  window.userToken = "<%= assigns[:user_token] %>";

  <%= if @conn.private.plug_session["user_id"] == nil do %>

    window.currentUser = "lobby";

  <% else %>

    window.currentUser = '<%=  @conn.private.plug_session["user_id"] %>';
  <% end %>

  <%= cond do %>

  <% get_flash(@conn, :info) != nil -> %>

  swal(
    '<%= get_flash(@conn, :info) %>',
    '',
    'success'
  )
  <% get_flash(@conn, :error) != nil -> %>

  swal(
    '<%= get_flash(@conn, :error) %>',
    '',
    'error'
  )

  <% true -> %>

  <% end %>

</script>

<script src="/js/app.js"></script>
<script src="/js/phoenix.js"></script>
<script src="/js/socket.js"></script>
