  <div class="form-group">
    <%= label f, :branchcode, class: "control-label" %>
    <%= text_input f, :branchcode, class: "form-control" %>
    <%= error_tag f, :branchcode %>
  </div>

  <div class="form-group">
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
